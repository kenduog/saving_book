<body>


    <aside class="sidebar">
        <div class="toggle">
            <a href="#" class="burger js-menu-toggle" data-toggle="collapse" data-target="#main-navbar">
                <span></span>
            </a>
        </div>
        <div class="side-inner">

            <div class="profile">
                <img src="images/person_profile.jpg" alt="Image" class="img-fluid">
                <h3 class="name">
                    <%=getUser.firstName%>
                        <%=getUser.lastName%>
                </h3>
                <span class="country">
                    <%=getUser.email%>
                </span>
            </div>

            <div class="counter d-flex justify-content-center">
                <!-- <div class="row justify-content-center"> -->
                <div class="col">
                    <strong class="number">
                        <%=listSavingBook.totalMoneyStr%>
                    </strong>
                    <span class="number-label">Total</span>
                </div>
                <div class="col">
                    <strong class="number">
                        <%=listSavingBook.decNECStr%>
                    </strong>
                    <span class="number-label">Need</span>
                </div>
                <div class="col">
                    <strong class="number">
                        <%=listSavingBook.decPLAYStr%>
                    </strong>
                    <span class="number-label">Play</span>
                </div>
                <!-- </div> -->
            </div>

            <div class="nav-menu">
                <ul>
                    <li class="active">
                        <form action="/" method="get">
                            <a href="#">
                                <span class="icon-home mr-3"></span>
                                <button class="form_nav" type="submit">Dashboard</button>
                            </a>
                        </form>
                    </li>
                    <!-- <li><a href="#"><span class="icon-notifications mr-3"></span>Notifications</a></li> -->
                    <li>
                        <form action="/logout" method="get">
                            <a>
                                <span class="icon-sign-out mr-3"></span>
                                <button type="submit" class="form_nav">Log out</button>
                            </a>
                        </form>
                    </li>
                </ul>
            </div>
        </div>

    </aside>

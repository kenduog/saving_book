<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link rel="stylesheet" href="/fonts/style.css">

    <link rel="stylesheet" href="/css/dashboard/owl.carousel.min.css">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="/css/dashboard/bootstrap.min.css">

    <!-- Style -->
    <link rel="stylesheet" href="/css/dashboard/style.css">
    <link rel="stylesheet" href="/css/dashboard/styleCard.css">

    <title>Dashboard</title>
</head>

<body>
    <header>
        <aside class="sidebar">
            <div class="toggle">
                <a href="#" class="burger js-menu-toggle" data-toggle="collapse" data-target="#main-navbar">
                    <span></span>
                </a>
            </div>
            <div class="side-inner">

                <div class="profile">
                    <img src="/img/none_avt.png" alt="Image" class="img-fluid">
                    <h3 class="name">
                        <%=getUser.firstName%>
                            <%=getUser.lastName%>
                    </h3>
                    <span class="country">
                        <%=getUser.email%>
                    </span>
                </div>

                <div class="counter d-flex justify-content-center">
                    <!-- <div class="row justify-content-center"> -->
                    <div class="col">
                        <strong class="number">
                            <%=listSavingBook.totalMoneyStr%>
                        </strong>
                        <span class="number-label">Total</span>
                    </div>
                    <div class="col">
                        <strong class="number">
                            <%=listSavingBook.decNECStr%>
                        </strong>
                        <span class="number-label">Need</span>
                    </div>
                    <div class="col">
                        <strong class="number">
                            <%=listSavingBook.decPLAYStr%>
                        </strong>
                        <span class="number-label">Play</span>
                    </div>
                    <!-- </div> -->
                </div>

                <div class="nav-menu">
                    <ul>
                        <li class="active">
                            <form action="/" method="get">
                                <a href="#">
                                    <span class="icon-home mr-3"></span>
                                    <button class="form_nav" type="submit">Dashboard</button>
                                </a>
                            </form>
                        </li>
                        <!-- <li><a href="#"><span class="icon-notifications mr-3"></span>Notifications</a></li> -->
                        <li>
                            <form action="/logout" method="get">
                                <a>
                                    <span class="icon-sign-out mr-3"></span>
                                    <button type="submit" class="form_nav">Log out</button>
                                </a>
                            </form>
                        </li>
                    </ul>
                </div>
            </div>

        </aside>
        <!-- partial:index.partial.html -->
        <nav class="nav">
            <ul>
                <li><a href="#">Total:<a></li>
                <li>
                    <a href="#">
                        <%=listSavingBook.totalMoney%>
                    </a>
                </li>
            </ul>
        </nav>
    </header>
    <div class="title-body">
        <h3>Quy Tắc 6 Chiếc Lọ</h3>
    </div>
    <div class="container">
        <section>
            <div class=" row">
                <div class="col-md-4 col-sm-6">
                    <div class="our-team">
                        <h3 class="title">CHI TIÊU CẦN THIẾT</h3>
                        <div class="pic">
                            <img src="/img/6-Bottle/NEC.jpg">
                        </div>
                        <h3 class="title">
                            <%=listSavingBook.decNEC%>
                        </h3>
                        <span class="post">LỌ SỐ 1: CHI TIÊU CẦN THIẾT - NEC</span>
                        <span class="post_percent">(55% THU NHẬP)</span>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="our-team">
                        <h3 class="title">TIẾT KIỆM DÀI HẠN</h3>
                        <div class="pic">
                            <img src="/img/6-Bottle/LTS.jpg">
                        </div>
                        <h3 class="title">
                            <%=listSavingBook.decLTS%>
                        </h3>
                        <span class="post">LỌ SỐ 2: TIẾT KIỆM DÀI HẠN - LTS
                        </span>
                        <span class="post_percent">(10% THU NHẬP)</span>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="our-team">
                        <h3 class="title">QUỸ GIÁO DỤC</h3>
                        <div class="pic">
                            <img src="/img/6-Bottle/EDU.jpg">
                        </div>
                        <h3 class="title">
                            <%=listSavingBook.decEDU%>
                        </h3>
                        <span class="post">LỌ SỐ 3: QUỸ GIÁO DỤC - EDU
                        </span>
                        <span class="post_percent">(10% THU NHẬP)</span>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="our-team">
                        <h3 class="title">HƯỞNG THỤ</h3>
                        <div class="pic">
                            <img src="/img/6-Bottle/PLAY.jpg">
                        </div>
                        <h3 class="title">
                            <%=listSavingBook.decPLAY%>
                        </h3>
                        <span class="post">LỌ SỐ 4: HƯỞNG THỤ - PLAY</span>
                        <span class="post_percent">(10% THU NHẬP)</span>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="our-team">
                        <h3 class="title">QUỸ TỰ DO TÀI CHÍNH</h3>
                        <div class="pic">
                            <img src="/img/6-Bottle/FFA.jpg">
                        </div>
                        <h3 class="title">
                            <%=listSavingBook.decFFA%>
                        </h3>
                        <span class="post">LỌ SỐ 5: QUỸ TỰ DO TÀI CHÍNH - FFA
                        </span>
                        <span class="post_percent">(10% THU NHẬP)</span>
                    </div>
                </div>

                <div class="col-md-4 col-sm-6">
                    <div class="our-team">
                        <h3 class="title">QUỸ TỪ THIỆN</h3>
                        <div class="pic">
                            <img src="/img/6-Bottle/GIVE.jpg">
                        </div>
                        <h3 class="title">
                            <%=listSavingBook.decGIVE%>
                        </h3>
                        <span class="post">QUỸ TỪ THIỆN - GIVE
                        </span>
                        <span class="post_percent">(5% THU NHẬP)</span>
                    </div>
                </div>
            </div>
        </section>
        <!-- partial -->
    </div>
    <script src="/js/jquery-3.3.1.min.js"></script>
    <script src="/js/popper.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/main.js"></script>
</body>

</html>